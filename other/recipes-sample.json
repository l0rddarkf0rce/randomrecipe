    {
        "title": "Coconut Curry Fried Rice",
        "ingredients" : [
            {
                "ingredient": "garlic",
                "quantity": "3",
                "measure": "cloves"
            },
            {
                "ingredient": "red onion",
                "quantity": "1/2"
            },
            {
                "ingredient": "bell pepper",
                "quantity": "1/2"
            },
            {
                "ingredient": "broccolini",
                "quantity": "70",
                "measure": "grams"
            },
            {
                "ingredient": "shredded coconut",
                "quantity": "1",
                "measure": "cup"
            },
            {
                "ingredient": "chili oil",
                "quantity": "2",
                "measure": "tablespoons"
            },
            {
                "ingredient": "chickpeas",
                "quantity": "1/2",
                "measure": "cup"
            },
            {
                "ingredient": "roasted unsalted peanuts",
                "quantity": "1/4",
                "measure": "cup"
            },
            {
                "ingredient": "leftover rice",
                "quantity": "1.5",
                "measure": "cups"
            },
            {
                "ingredient": "curry powder",
                "quantity": "1",
                "measure": "tablespoon"
            },
            {
                "ingredient": "soy sauce",
                "quantity": "2",
                "measure": "tablespoons"
            }
        ],
        "steps" : {
            "1": "Roughly chop the garlic, dice the red onion and bell pepper, and chop the broccolini into bite size pieces",
            "2": "Heat up a non stick pan to medium heat.  Toast the shredded coconut for 1-2 minutes and set aside",
            "3": "Place pan back over medium heat",
            "4": "Add in the chili oil followed by the red onions and garlic and sauté for 2 minutes",
            "5": "Add in the chickpeas, broccolini, and red bell pepper and sauté for 2-3 minutes",
            "6": "Add in the peanuts, rice, curry powder, and soy sauce",
            "7": "Sauté for another 2-3 minutes. Then, add in 2/3 of the toasted coconut and save the rest for garnish",
            "8": "Give the rice a stir around and plate. Garnish with the remaining toasted coconut and serve"
        }
    },
